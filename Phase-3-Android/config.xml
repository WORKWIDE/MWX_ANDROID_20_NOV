<?xml version='1.0' encoding='utf-8'?>
<widget id="com.quintica.workwide_dev1_0_8" version="1.10" versionCode="16" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <gap:config-file parent="CFBundleVersion" platform="ios">
        <string>1.11</string>
    </gap:config-file>
    <name>WorkWide</name>
    <description>
        WorkWide
    </description>
    <author email="info@quintica.com" href="http://quintica.com/">
        WorkWide
    </author>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ActionSheet">
        <param name="ios-package" value="ActionSheet" />
    </feature>
    <feature name="BackgroundMode">
        <param name="ios-package" value="APPBackgroundMode" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <gap:config-file platform="ios" parent="NSPhotoLibraryUsageDescription" overwrite="true">
    <string>We are using the Photo Library</string>
    </gap:config-file>
    <gap:config-file platform="ios" parent="NSCameraUsageDescription" overwrite="true">
    <string>We are using the Camera</string>
    </gap:config-file>
     <engine name="android" spec="7.0.0" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-camera" spec="2.3.1" />
    <plugin name="cordova-plugin-geolocation" spec="2.4.1" />
    <plugin name="cordova-plugin-file" spec="4.3.1" />
    <plugin name="cordova-plugin-device" spec="2.0.1" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-actionsheet" spec="2.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.1" />
    <plugin name="phonegap-plugin-push" spec="1.10.5">
        <variable name="SENDER_ID" value="535739481079" />
    </plugin>
     <plugin name="cordova-plugin-dialogs" spec="2.0.1" />
     <plugin name="cordova-plugin-request-location-accuracy" spec="2.2.2" />
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <platform name="android" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="FullScreen" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="target-device" value="universal" />
    <preference name="prerendered-icon" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="useBrowserHistory" value="true" />
    <icon src="icon.png" />
    <platform name="android">
    <icon qualifier="ldpi" platform="android" src="res/icons/android/drawable-ldpi-icon.png" />
    <icon qualifier="mdpi" platform="android" src="res/icons/android/drawable-mdpi-icon.png" />
    <icon qualifier="hdpi" platform="android" src="res/icons/android/drawable-hdpi-icon.png" />
    <icon qualifier="xhdpi" platform="android" src="res/icons/android/drawable-xhdpi-icon.png" />
    <icon qualifier="xxhdpi" platform="android" src="res/icons/android/drawable-xxhdpi-icon.png" />
    <icon qualifier="xxxhdpi" platform="android" src="res/icons/android/drawable-xxxhdpi-icon.png" />
    <splash qualifier="land-ldpi"  platform="android"  src="res/screens/android/drawable-land-ldpi-screen.png" />
    <splash qualifier="land-mdpi"  platform="android"  src="res/screens/android/drawable-land-mdpi-screen.png" />
    <splash qualifier="land-hdpi" platform="android" src="res/screens/android/drawable-land-hdpi-screen.png" />
    <splash qualifier="land-xhdpi"  platform="android"  src="res/screens/android/drawable-land-xhdpi-screen.png" />
    <splash qualifier="land-xxhdpi"  platform="android"  src="res/screens/android/drawable-land-xxhdpi-screen.png" />
    <splash qualifier="land-xxxhdpi"  platform="android"  src="res/screens/android/drawable-land-xxxhdpi-screen.png" />
    <splash qualifier="port-ldpi"  platform="android"  src="res/screens/android/drawable-port-ldpi-screen.png" />
    <splash density="port-mdpi"  platform="android"  src="res/screens/android/drawable-port-mdpi-screen.png" />
    <splash qualifier="port-hdpi"  platform="android"  src="res/screens/android/drawable-port-hdpi-screen.png" />
    <splash qualifier="port-xhdpi"  platform="android"  src="res/screens/android/drawable-port-xhdpi-screen.png" />
    <splash qualifier="port-xxhdpi"  platform="android"  src="res/screens/android/drawable-port-xxhdpi-screen.png" />
    <splash qualifier="port-xxxhdpi"  platform="android"  src="res/screens/android/drawable-port-xxxhdpi-screen.png" />
  </platform>
</widget>
